                </div>
            </div>
            <div class="vis-ad-w-p-actionpanel-r">
                @* Tab Panel*@
            </div>
        </div>
        <div class="vis-ad-w-p-action">
            <div class="vis-ad-w-a-arrow">
                <i class="fa fa-sort-down"></i>
            </div>
            <div class="vis-ad-w-p-a-main">
                <ul class="vis-ad-w-p-a-m-lst vis-ad-w-p-a-m-dlist" style="flex: initial;">
