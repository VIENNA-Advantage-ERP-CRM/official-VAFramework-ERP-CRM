        <div class="vis-thed-clrpickerouterwrap">
            <fieldset>
                <legend>@VAdvantage.Utility.Msg.GetMsg((string)ViewBag.culture, "ChooseYourColor")</legend>
                <div class="vis-thed-clrpickerwrap">
                    <div class="vis-thed-clrpickersec">
                        <div class="vis-thed-clrpickerlistitem">
                            <div class="vis-thed-clrpickerbox">
                                <input type="color" name="primary" value="#0093fc">
                            </div><!-- vis-thed-clrpickerbox -->
                            <div class="vis-thed-clrname">
                                <p>@VAdvantage.Utility.Msg.GetMsg((string)ViewBag.culture, "Primary")<i class="vis vis-info"></i></p>
                            </div><!-- vis-thed-clrname -->
                        </div><!-- vis-thed-clrpickerlistitem -->

                        <div class="vis-thed-clrpickerlistitem">
                            <div class="vis-thed-clrpickerbox">
                                <input type="color" name="onprimary" value="#ffffff">
                            </div><!-- vis-thed-clrpickerbox -->
                            <div class="vis-thed-clrname">
                                <p>@VAdvantage.Utility.Msg.GetMsg((string)ViewBag.culture, "OnPrimary")<i class="vis vis-info"></i></p>
                            </div><!-- vis-thed-clrname -->
                        </div><!-- vis-thed-clrpickerlistitem -->
                    </div><!-- vis-thed-clrpickersec -->

                    <div class="vis-thed-clrpickersec">
                        <div class="vis-thed-clrpickerlistitem">
                            <div class="vis-thed-clrpickerbox">
                                <input type="color" name="secondary" value="#eeeeee">
                            </div><!-- vis-thed-clrpickerbox -->
