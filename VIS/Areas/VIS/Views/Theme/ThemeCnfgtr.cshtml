                    <li style="opacity: 0.6;"><i class="vis vis-save"></i></li>
                    <li><i class="vis vis-refresh"></i></li>
                    <li><i class="vis vis-report"></i></li>
                    <li style="opacity: 0.6;"><i class="vis vis-print"></i></li>
                </ul>
                <div class="vis-ad-w-p-tb-search">
