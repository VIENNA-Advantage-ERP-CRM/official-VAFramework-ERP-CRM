        <div class="vis-thed-botsavesec">
            <div class="vis-input-wrap vis-thed-themenameinput">
                <input type="text" name="" placeholder="@VAdvantage.Utility.Msg.GetMsg((string)ViewBag.culture, "EnterThemeName")">
                <label>@VAdvantage.Utility.Msg.GetMsg((string)ViewBag.culture, "Name")</label>
            </div><!-- vis-thed-themenameinput -->
            <div class="vis-thed-btnswrap">
                <a class="btn vis-thed-btn" data-action="save">@VAdvantage.Utility.Msg.GetMsg((string)ViewBag.culture, "SaveTheme")</a>
                <a class="btn vis-thed-btn" data-action="sandd">@VAdvantage.Utility.Msg.GetMsg((string)ViewBag.culture, "Save&SetDefault")</a>
            </div>
        </div><!-- vis-thed-botsavesec -->
    </div><!-- vis-thed-contentsec -->

